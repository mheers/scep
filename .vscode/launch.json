    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Package",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/scepclient",
            "args": [
                "-server-url=http://127.0.0.1:2016/scep",
                "-challenge=secret",
                "-private-key",
                "client.key",
                "-cn",
                "my-client",
                "-country",
                "DE",
                "-debug",
                "-organization",
                "acme-scep-ca",
                "-ou",
                "ACME CA",
            ]
        }
    ]
}